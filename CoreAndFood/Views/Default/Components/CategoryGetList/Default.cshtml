@model List<Category>


<div>
    <ul class="nav navbar-nav nav_1">
        @foreach (var item in Model)
        {
            <li><a asp-action="CategoryDetails" asp-controller="Default" asp-route-id="@item.CategoryId">@item.CategoryName</a></li>
        }
    </ul>
</div>
@* href="/Default/CategoryDetails/@item.CategoryId" *@